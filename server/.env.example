# Copy to .env and set values. Do not commit .env.
PORT=4000
# Secret for JWT signing (use a long random string in production)
JWT_SECRET=your_jwt_secret_change_in_production
# Allowed frontend origin (e.g. http://localhost:3000)
CORS_ORIGIN=http://localhost:3000
# Public URL for the API (used for iCal links)
API_PUBLIC_URL=http://localhost:4000
